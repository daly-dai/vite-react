{
  "compilerOptions": {
    "jsx": "react",
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      // 根据别名配置相关路径
      "@": ["src"],
      "@/*": ["src/*"],
      "@packages/*": ["src/packages/*"],
      "@pages/*": ["src/pages/*"],
      "@common/*": ["src/common/*"],
      "@hooks/*": ["src/hooks/*"],
      "@store/*": ["src/store/*"],
      "@utils/*": ["src/utils/*"],
      "@config/*": ["src/config/*"],
      "@router/*": ["src/router/*"],
      "@service/*": ["src/service/*"],
      "@types/*": ["src/types/*"],
      "@components/*": ["src/packages/components/*"],
      "@plugins/*": ["src/plugins/*"]
    }
  },
  "include": [
    "vite.config.ts",
    // "src/**/*",
    // "app.config.ts",
    // "config/**/*",
  ]
}
